#include <stdio.h>
int main() {
    int a = 13, b= 3;

    // Arithmetic Operataors
    printf("Arithmetic Operataors\n");
    printf("%d + %d = %d\n", a, b, a+b);
    printf("%d - %d = %d\n", a, b, a-b);
    printf("%d * %d = %d\n", a, b, a*b);
    printf("%d / %d = %d\n", a, b, (a/b));
    printf("%d %% %d = %d\n\n", a, b, a%b);
     (a < b) ? printf("\na is greater than b"): printf("\nb is less than a");

    return 0;}

OUTPUT***
Arithmetic Operataors
13 + 3 = 16
13 - 3 = 10
13 * 3 = 39
13 / 3 = 4
13 % 3 = 1


b is less than a

=== Code Execution Successful ===
